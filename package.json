{
  "name": "aurelia-router-metadata",
  "version": "0.6.0",
  "description": "Adds decorator configuration with conventions and eager loading capabilities to aurelia-router",
  "keywords": [
    "aurelia",
    "router",
    "metadata",
    "decorator",
    "eager"
  ],
  "homepage": "https://github.com/fkleuver/aurelia-router-metadata",
  "bugs": {
    "url": "https://github.com/fkleuver/aurelia-router-metadata/issues"
  },
  "license": "MIT",
  "author": "Fred Kleuver <fred@avurad.nl>",
  "main": "dist/commonjs/aurelia-router-metadata.js",
  "typings": "dist/commonjs/aurelia-router-metadata.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fkleuver/aurelia-router-metadata"
  },
  "scripts": {
    "lint": "cross-env tslint --project configs/tsconfig-build.json",
    "test": "cross-env karma start --single-run",
    "dev": "cross-env karma start",
    "prebuild": "cross-env npm run lint && rimraf dist",
    "build:amd": "cross-env tsc --project configs/tsconfig-build-amd.json",
    "build:commonjs": "cross-env tsc --project configs/tsconfig-build-commonjs.json",
    "build:es2017": "cross-env tsc --project configs/tsconfig-build-es2017.json",
    "build:es2015": "cross-env tsc --project configs/tsconfig-build-es2015.json",
    "build:native-modules": "cross-env tsc --project configs/tsconfig-build-native-modules.json",
    "build:system": "cross-env tsc --project configs/tsconfig-build-system.json",
    "build": "concurrently \"npm run build:amd\" \"npm run build:commonjs\" \"npm run build:es2015\" \"npm run build:es2017\" \"npm run build:native-modules\" \"npm run build:system\"",
    "predoc": "cross-env rimraf doc/api.json && cross-env rimraf dist/doc-temp && cross-env tsc --project configs/tsconfig-build-amd.json --outFile dist/doc-temp/${npm_package_name}.js --module amd && node doc/shape-defs && copyfiles configs/tsconfig.json dist/doc-temp",
    "doc": "cross-env typedoc --json doc/api.json --excludeExternals --includeDeclarations --mode modules --target ES6 --name ${npm_package_name}-docs dist/doc-temp/",
    "postdoc": "cross-env node doc/shape-doc && rimraf dist/doc-temp",
    "changelog": "cross-env conventional-changelog -p angular -i doc/CHANGELOG.md -s",
    "bump-version": "npm --no-git-tag-version version",
    "preprepare-release": "cross-env npm run test",
    "prepare-release": "cross-env npm run changelog && npm run build && npm run doc"
  },
  "dependencies": {
    "aurelia-dependency-injection": "^1.3.2",
    "aurelia-loader": "^1.0.0",
    "aurelia-logging": "^1.4.0",
    "aurelia-metadata": "^1.0.3",
    "aurelia-pal": "^1.5.0",
    "aurelia-router": "^1.5.0"
  },
  "devDependencies": {
    "@types/bluebird-global": "^3.5.5",
    "@types/jasmine": "^2.8.6",
    "@types/karma": "^1.7.3",
    "@types/node": "^9.4.6",
    "@types/webpack": "^3.8.8",
    "aurelia-pal-browser": "^1.4.0",
    "aurelia-polyfills": "^1.3.0",
    "aurelia-webpack-plugin": "^3.0.0-rc.1",
    "bluebird": "^3.5.1",
    "concurrently": "^3.5.1",
    "conventional-changelog-cli": "^1.3.15",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.1.3",
    "expose-loader": "^0.7.4",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^2.0.13",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
  },
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "main": "index",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "peerDependencies": {
      "aurelia-dependency-injection": "^1.3.2",
      "aurelia-loader": "^1.0.0",
      "aurelia-logging": "^1.4.0",
      "aurelia-metadata": "^1.0.3",
      "aurelia-pal": "^1.5.0",
      "aurelia-router": "^1.5.0"
    },
    "dependencies": {
      "aurelia-dependency-injection": "^1.3.2",
      "aurelia-loader": "^1.0.0",
      "aurelia-logging": "^1.4.0",
      "aurelia-metadata": "^1.0.3",
      "aurelia-pal": "^1.5.0",
      "aurelia-router": "^1.5.0"
    }
  },
  "aurelia": {
    "import": {
      "dependencies": [
        {
          "name": "aurelia-router-metadata",
          "path": "../node_modules/aurelia-router-metadata/dist/amd",
          "main": "aurelia-router-metadata"
        }
      ]
    }
  }
}
